{
  "name": "leomaglove-be",
  "version": "1.0.0",
  "description": "leomaglove.be",
  "repository": "https://github.com/mathieux51/leomaglove-be",
  "main": "index.js",
  "scripts": {
    "analyze:all": "BUNDLE_ANALYZE=all npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:web": "BUNDLE_ANALYZE=web npm run build",
    "build": "next build",
    "clean": "rimraf node_modules/.cache",
    "cname": "touch out/CNAME && echo \"leomaglove.be\\n\" >> out/CNAME",
    "deploy": "git add out/ && git commit -m \"Deploy to gh-pages\" && git push origin `git subtree split --prefix out master`:gh-pages --force && git push",
    "dev": "next",
    "export": "next export",
    "predeploy": "npm run clean && npm run build && npm run export && touch out/.nojekyll && npm run cname",
    "start": "next start"
  },
  "keywords": [],
  "author": "Mathieu Schmidt",
  "license": "ISC",
  "dependencies": {
    "next": "^8.0.1",
    "polished": "^3.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hotjar": "^2.0.0",
    "react-plyr": "^2.1.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "dotenv-webpack": "^1.7.0",
    "next-fonts": "^0.17.0",
    "next-images": "^1.1.0",
    "rimraf": "^2.6.3"
  }
}
